{
    "installation": {
        "installer_command": "npm install"
    },
    "build": {
        "build_script_list": [
            "prebuild",
            "build",
            "build:docs"
        ]
    },
    "testing": {
        "test": {
            "nested_test_commands": [
                "lint"
            ],
            "RUNS_NEW_USER_TESTS": false,
            "timed_out": false
        },
        "lint": {
            "test_linters": [
                "standard -- linter"
            ],
            "RUNS_NEW_USER_TESTS": false,
            "timed_out": false
        },
        "lint:fix": {
            "test_linters": [
                "standard -- linter"
            ],
            "RUNS_NEW_USER_TESTS": false,
            "timed_out": false
        }
    },
    "metadata": {
        "repo_link": "https://github.com/xiaody/react-touch-carousel"
    }
}