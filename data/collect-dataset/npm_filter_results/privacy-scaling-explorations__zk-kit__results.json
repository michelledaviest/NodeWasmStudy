{
    "installation": {
        "installer_command": "yarn"
    },
    "build": {
        "build_script_list": [
            "build",
            "build:js",
            "compile:sol"
        ]
    },
    "testing": {
        "test": {
            "nested_test_commands": [
                "test:js",
                "test:sol"
            ],
            "RUNS_NEW_USER_TESTS": false,
            "timed_out": false
        },
        "test:js": {
            "num_passing": 90,
            "num_failing": 0,
            "test_infras": [
                "jest"
            ],
            "timed_out": false
        },
        "test:sol": {
            "RUNS_NEW_USER_TESTS": false,
            "timed_out": false
        },
        "lint": {
            "test_linters": [
                "eslint -- linter"
            ],
            "RUNS_NEW_USER_TESTS": false,
            "timed_out": false
        },
        "benchmarks": {
            "RUNS_NEW_USER_TESTS": false,
            "timed_out": false
        }
    },
    "metadata": {
        "repo_link": "https://github.com/privacy-scaling-explorations/zk-kit"
    }
}