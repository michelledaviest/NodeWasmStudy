{
    "installation": {
        "installer_command": "yarn"
    },
    "build": {
        "build_script_list": [
            "rebuild",
            "build",
            "build-nopack",
            "build-tsc",
            "build-configSchema",
            "build-pack",
            "build-manifest"
        ]
    },
    "testing": {
        "lint": {
            "RUNS_NEW_USER_TESTS": false,
            "timed_out": false
        },
        "test-spec": {
            "ERROR": true,
            "num_passing": 0,
            "num_failing": 1,
            "test_infras": [
                "ava"
            ],
            "timed_out": false
        },
        "test-cov": {
            "ERROR": true,
            "num_passing": 0,
            "num_failing": 1,
            "test_infras": [
                "ava"
            ],
            "test_coverage_tools": [
                "nyc -- coverage testing"
            ],
            "timed_out": false
        },
        "test": {
            "ERROR": true,
            "nested_test_commands": [
                "lint",
                "test-cov"
            ],
            "RUNS_NEW_USER_TESTS": false,
            "timed_out": false
        },
        "test-local": {
            "ERROR": true,
            "nested_test_commands": [
                "test-spec"
            ],
            "RUNS_NEW_USER_TESTS": false,
            "timed_out": false
        },
        "coverage-report": {
            "test_coverage_tools": [
                "nyc -- coverage testing"
            ],
            "RUNS_NEW_USER_TESTS": false,
            "timed_out": false
        }
    },
    "metadata": {
        "repo_link": "https://github.com/TypeStrong/ts-node"
    }
}