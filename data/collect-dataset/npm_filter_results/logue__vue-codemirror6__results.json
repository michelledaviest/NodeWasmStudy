{
    "installation": {
        "installer_command": "yarn"
    },
    "build": {
        "build_script_list": [
            "build",
            "build:analyze",
            "build:clean",
            "build:docs",
            "build-only",
            "build-declaration"
        ]
    },
    "testing": {
        "lint": {
            "test_linters": [
                "eslint -- linter",
                "prettier -- linter"
            ],
            "RUNS_NEW_USER_TESTS": false,
            "timed_out": false
        }
    },
    "metadata": {
        "repo_link": "https://github.com/logue/vue-codemirror6"
    }
}