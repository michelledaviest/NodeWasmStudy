{
    "installation": {
        "installer_command": "npm install"
    },
    "build": {
        "build_script_list": [
            "build",
            "build:package"
        ],
        "ERROR": true
    },
    "testing": {
        "test:unit": {
            "ERROR": true,
            "RUNS_NEW_USER_TESTS": false,
            "timed_out": false
        },
        "test:coverage": {
            "ERROR": true,
            "RUNS_NEW_USER_TESTS": false,
            "timed_out": false
        },
        "lint": {
            "test_linters": [
                "eslint -- linter"
            ],
            "RUNS_NEW_USER_TESTS": false,
            "timed_out": false
        },
        "lint:fix": {
            "test_linters": [
                "eslint -- linter"
            ],
            "RUNS_NEW_USER_TESTS": false,
            "timed_out": false
        }
    },
    "metadata": {
        "repo_link": "https://github.com/primefaces/primevue"
    }
}