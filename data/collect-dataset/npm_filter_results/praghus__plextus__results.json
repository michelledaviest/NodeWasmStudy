{
    "installation": {
        "installer_command": "yarn"
    },
    "build": {
        "build_script_list": [
            "build"
        ]
    },
    "testing": {
        "test": {
            "ERROR": true,
            "num_passing": 0,
            "num_failing": 2,
            "test_infras": [
                "jest"
            ],
            "timed_out": false
        },
