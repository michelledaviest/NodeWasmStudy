{
    "installation": {
        "installer_command": "npm install"
    },
    "build": {
        "build_script_list": [
            "build",
            "types:build"
        ]
    },
    "testing": {
        "test": {
            "ERROR": true,
            "num_passing": 0,
            "num_failing": 4,
            "test_infras": [
                "ava"
            ],
            "timed_out": false
        },
        "lint": {
            "test_linters": [
                "eslint -- linter"
            ],
            "RUNS_NEW_USER_TESTS": false,
            "timed_out": false
        },
        "lint:fix": {
            "nested_test_commands": [
                "lint"
            ],
            "RUNS_NEW_USER_TESTS": false,
            "timed_out": false
        }
    },
    "metadata": {
        "repo_link": "https://github.com/cloudflare/miniflare"
    }
}