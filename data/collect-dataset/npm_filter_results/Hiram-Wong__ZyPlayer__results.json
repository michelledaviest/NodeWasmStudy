{
    "installation": {
        "installer_command": "yarn"
    },
    "build": {
        "build_script_list": [
            "build",
            "electron:build",
            "electron:build:linux",
            "electron:prebuild"
        ],
        "ERROR": true
    },
    "testing": {
        "lint": {
            "ERROR": true,
            "test_linters": [
                "eslint -- linter"
            ],
            "RUNS_NEW_USER_TESTS": false,
            "timed_out": false
        },
        "lint:fix": {
            "ERROR": true,
            "test_linters": [
                "eslint -- linter"
            ],
            "RUNS_NEW_USER_TESTS": false,
            "timed_out": false
        }
    },
    "metadata": {
        "repo_link": "https://github.com/Hiram-Wong/ZyPlayer"
    }
}