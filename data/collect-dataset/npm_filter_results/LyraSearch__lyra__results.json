{
    "installation": {
        "installer_command": "npm install"
    },
    "build": {
        "build_script_list": [
            "prebuild",
            "build"
        ],
        "ERROR": true
    },
    "testing": {
        "lint": {
            "test_linters": [
                "eslint -- linter"
            ],
            "RUNS_NEW_USER_TESTS": false,
            "timed_out": false
        },
        "test": {
            "test_coverage_tools": [
                "c8 -- coverage testing"
            ],
            "RUNS_NEW_USER_TESTS": false,
            "timed_out": false
        },
        "test:ci": {
            "test_coverage_tools": [
                "c8 -- coverage testing"
            ],
            "RUNS_NEW_USER_TESTS": false,
            "timed_out": false
        },
        "test:smoke": {
            "num_passing": 0,
            "num_failing": 0,
            "test_infras": [
                "tap"
            ],
            "timed_out": false
        },
        "ci": {
            "nested_test_commands": [
                "test:ci",
                "test:smoke"
            ],
            "RUNS_NEW_USER_TESTS": false,
            "timed_out": false
        },
        "benchmark": {
            "ERROR": true,
            "num_passing": 0,
            "num_failing": 0,
            "test_infras": [
                "Custom Testing: node"
            ],
            "timed_out": false
        }
    },
    "metadata": {
        "repo_link": "https://github.com/LyraSearch/lyra"
    }
}