        "build_script_list": [
            "about:build",
            "build"
        ]
    },
    "testing": {
        "about:eslint": {
            "RUNS_NEW_USER_TESTS": false,
            "timed_out": false
        },
        "about:test": {
            "RUNS_NEW_USER_TESTS": false,
            "timed_out": false
        },
        "about:coverage": {
            "RUNS_NEW_USER_TESTS": false,
            "timed_out": false
        },
        "eslint": {
            "test_linters": [
                "eslint -- linter"
