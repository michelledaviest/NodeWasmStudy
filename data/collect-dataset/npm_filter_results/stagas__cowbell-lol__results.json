{
    "installation": {
        "installer_command": "npm install"
    },
    "build": {
        "build_script_list": [
            "build:dist",
            "build:cjs",
            "build:esm",
            "build:bundle",
            "build:min"
        ],
        "ERROR": true
    },
    "testing": {
        "test": {
            "RUNS_NEW_USER_TESTS": false,
            "timed_out": false
        },
        "cov": {
            "RUNS_NEW_USER_TESTS": false,
            "timed_out": false
        },
        "lint": {
            "test_linters": [
                "eslint -- linter"
            ],
            "RUNS_NEW_USER_TESTS": false,
            "timed_out": false
        },
        "lint:fix": {
            "test_linters": [
                "eslint -- linter"
            ],
            "RUNS_NEW_USER_TESTS": false,
            "timed_out": false
        }
    },
    "metadata": {
        "repo_link": "https://github.com/stagas/cowbell-lol"
    }
}