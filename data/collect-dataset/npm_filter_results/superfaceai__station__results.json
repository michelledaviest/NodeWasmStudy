{
    "installation": {
        "installer_command": "yarn"
    },
    "build": {
        "build_script_list": [
            "compile",
            "compile:clear"
        ]
    },
    "testing": {
        "tools:test": {
            "num_passing": 30,
            "num_failing": 0,
            "test_infras": [
                "jest"
            ],
            "timed_out": false
        },
        "tools:lint": {
            "test_linters": [
