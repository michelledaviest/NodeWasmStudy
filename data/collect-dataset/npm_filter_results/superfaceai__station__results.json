        "test": {
            "ERROR": true,
            "RUNS_NEW_USER_TESTS": false,
            "timed_out": false
        },
        "test:record": {
            "ERROR": true,
            "RUNS_NEW_USER_TESTS": false,
            "timed_out": false
        },
        "lint": {
            "RUNS_NEW_USER_TESTS": false,
            "timed_out": false
