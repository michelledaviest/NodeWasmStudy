{
    "installation": {
        "installer_command": "yarn"
    },
    "build": {
        "build_script_list": [
            "build",
            "build:r",
            "build:ts"
        ]
    },
    "testing": {
        "lint": {
            "RUNS_NEW_USER_TESTS": false,
            "timed_out": false
        },
        "lint:es": {
            "test_linters": [
                "eslint -- linter"
            ],
            "RUNS_NEW_USER_TESTS": false,
            "timed_out": false
        },
        "lint:tsc": {
            "RUNS_NEW_USER_TESTS": false,
            "timed_out": false
        },
        "test": {
            "num_passing": 1,
            "num_failing": 0,
            "test_infras": [
                "Custom Testing: node"
            ],
            "timed_out": false
        },
        "typecov": {
            "RUNS_NEW_USER_TESTS": false,
            "timed_out": false
        }
    },
    "metadata": {
        "repo_link": "https://github.com/un-ts/changesets-gitlab"
    }
}