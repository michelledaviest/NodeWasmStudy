{
    "installation": {
        "installer_command": "yarn"
    },
    "build": {
        "build_script_list": [
            "build:examples",
            "build:nbextension",
            "build:test"
        ],
        "ERROR": true
    },
    "testing": {
        "build:test": {
            "RUNS_NEW_USER_TESTS": false,
            "timed_out": false
        },
        "eslint": {
            "test_linters": [
                "eslint -- linter"
            ],
            "RUNS_NEW_USER_TESTS": false,
            "timed_out": false
        },
        "eslint:check": {
            "test_linters": [
                "eslint -- linter"
            ],
            "RUNS_NEW_USER_TESTS": false,
            "timed_out": false
        },
        "lint": {
            "nested_test_commands": [
                "eslint"
            ],
            "RUNS_NEW_USER_TESTS": false,
            "timed_out": false
        },
        "lint:check": {
            "nested_test_commands": [
                "eslint:check"
            ],
            "RUNS_NEW_USER_TESTS": false,
            "timed_out": false
        }
    },
    "metadata": {
        "repo_link": "https://github.com/jupyter-widgets/ipywidgets"
    }
}