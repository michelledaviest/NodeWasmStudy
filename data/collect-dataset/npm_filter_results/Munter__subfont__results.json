{
    "installation": {
        "installer_command": "npm install"
    },
    "build": {
        "build_script_list": []
    },
    "testing": {
        "lint": {
            "test_linters": [
                "eslint -- linter",
                "prettier -- linter"
            ],
            "RUNS_NEW_USER_TESTS": false,
            "timed_out": false
        },
        "test": {
            "num_passing": 250,
            "num_failing": 0,
            "test_infras": [
                "mocha"
            ],
            "nested_test_commands": [
                "lint"
            ],
            "timed_out": false
        },
        "travis": {
            "nested_test_commands": [
                "lint",
                "coverage"
            ],
            "RUNS_NEW_USER_TESTS": false,
            "timed_out": false
        },
        "coverage": {
            "test_coverage_tools": [
                "nyc -- coverage testing"
            ],
            "RUNS_NEW_USER_TESTS": false,
            "timed_out": false
        }
    },
    "metadata": {
        "repo_link": "https://github.com/Munter/subfont"
    }
}