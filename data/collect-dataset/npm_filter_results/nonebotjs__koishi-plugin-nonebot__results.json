{
    "installation": {
        "installer_command": "yarn"
    },
    "build": {
        "build_script_list": [
            "nbp:build",
            "docs:build",
            "build"
        ]
    },
    "testing": {
        "lint": {
            "test_linters": [
                "eslint -- linter"
            ],
            "RUNS_NEW_USER_TESTS": false,
            "timed_out": false
        },
        "test": {
            "ERROR": true,
            "RUNS_NEW_USER_TESTS": false,
            "timed_out": false
        },
        "test:text": {
            "ERROR": true,
            "test_coverage_tools": [
                "c8 -- coverage testing"
            ],
            "RUNS_NEW_USER_TESTS": false,
            "timed_out": false
        },
        "test:json": {
            "ERROR": true,
            "test_coverage_tools": [
                "c8 -- coverage testing"
            ],
            "RUNS_NEW_USER_TESTS": false,
            "timed_out": false
        },
        "test:html": {
            "ERROR": true,
            "test_coverage_tools": [
                "c8 -- coverage testing"
            ],
            "RUNS_NEW_USER_TESTS": false,
            "timed_out": false
        }
    },
    "metadata": {
        "repo_link": "https://github.com/nonebotjs/koishi-plugin-nonebot"
    }
}