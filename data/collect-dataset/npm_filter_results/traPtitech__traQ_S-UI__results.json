{
    "installation": {
        "installer_command": "npm install"
    },
    "build": {
        "build_script_list": [
            "build",
            "build:with-font",
            "build:may-with-font"
        ]
    },
    "testing": {
        "test:unit": {
            "RUNS_NEW_USER_TESTS": false,
            "timed_out": false
        },
        "test:e2e": {
            "ERROR": true,
            "nested_test_commands": [
                "test:e2e:only-run"
            ],
            "RUNS_NEW_USER_TESTS": false,
            "timed_out": false
        },
        "test:e2e-headless": {
            "ERROR": true,
            "nested_test_commands": [
                "test:e2e:only-run-headless"
            ],
            "RUNS_NEW_USER_TESTS": false,
            "timed_out": false
        },
        "test:e2e:only-run": {
            "ERROR": true,
            "RUNS_NEW_USER_TESTS": false,
            "timed_out": false
        },
        "test:e2e:only-run-headless": {
            "ERROR": true,
            "RUNS_NEW_USER_TESTS": false,
            "timed_out": false
        },
        "lint": {
            "test_linters": [
                "eslint -- linter"
            ],
            "RUNS_NEW_USER_TESTS": false,
            "timed_out": false
        },
        "lint:no-fix": {
            "test_linters": [
                "eslint -- linter"
            ],
            "RUNS_NEW_USER_TESTS": false,
            "timed_out": false
        },
        "type-check:test": {
            "RUNS_NEW_USER_TESTS": false,
            "timed_out": false
        }
    },
    "metadata": {
        "repo_link": "https://github.com/traPtitech/traQ_S-UI"
    }
}