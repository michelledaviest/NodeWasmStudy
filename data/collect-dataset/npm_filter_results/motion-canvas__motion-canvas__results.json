{
    "installation": {
        "installer_command": "npm install"
    },
    "build": {
        "build_script_list": [
            "core:build",
            "2d:build",
            "ui:build",
            "player:build",
            "docs:build",
            "vite-plugin:build",
            "template:build",
            "examples:build"
        ]
    },
    "testing": {
        "core:test": {
            "ERROR": true,
            "RUNS_NEW_USER_TESTS": false,
            "timed_out": false
        },
        "e2e:test": {
            "ERROR": true,
            "RUNS_NEW_USER_TESTS": false,
            "timed_out": false
        },
        "eslint": {
            "test_linters": [
                "eslint -- linter"
            ],
            "RUNS_NEW_USER_TESTS": false,
            "timed_out": false
        },
        "eslint:fix": {
            "test_linters": [
                "eslint -- linter"
            ],
            "RUNS_NEW_USER_TESTS": false,
            "timed_out": false
        }
    },
    "metadata": {
        "repo_link": "https://github.com/motion-canvas/motion-canvas"
    }
}