{
    "installation": {
        "installer_command": "npm install"
    },
    "build": {
        "build_script_list": [
            "build",
            "build:ci",
            "build:types"
        ]
    },
    "testing": {
        "build:ci": {
            "RUNS_NEW_USER_TESTS": false,
            "timed_out": false
        },
        "lint": {
            "test_linters": [
                "eslint -- linter"
            ],
            "RUNS_NEW_USER_TESTS": false,
            "timed_out": false
        },
        "lint:fix": {
            "nested_test_commands": [
                "lint"
            ],
            "RUNS_NEW_USER_TESTS": false,
            "timed_out": false
        },
        "lint:quiet": {
            "nested_test_commands": [
                "lint"
            ],
            "RUNS_NEW_USER_TESTS": false,
            "timed_out": false
        },
        "lint:staged": {
            "RUNS_NEW_USER_TESTS": false,
            "timed_out": false
        },
        "pretest": {
            "nested_test_commands": [
                "lint"
            ],
            "RUNS_NEW_USER_TESTS": false,
            "timed_out": false
        },
        "test": {
            "ERROR": true,
            "nested_test_commands": [
                "test:unit",
                "test:func"
            ],
            "RUNS_NEW_USER_TESTS": false,
            "timed_out": false
        },
        "test:unit": {
            "RUNS_NEW_USER_TESTS": false,
            "timed_out": false
        },
        "test:func": {
            "RUNS_NEW_USER_TESTS": false,
            "timed_out": false
        },
        "test:func:light": {
            "RUNS_NEW_USER_TESTS": false,
            "timed_out": false
        },
        "test:func:sauce": {
            "ERROR": true,
            "RUNS_NEW_USER_TESTS": false,
            "timed_out": false
        }
    },
    "metadata": {
        "repo_link": "https://github.com/video-dev/hls.js"
    }
}